# Multi-chain Asset Inventory Configuration
# Copy this file to .env and fill in your actual addresses

# Solana (Phantom Wallet)
SOLANA_ADDRESS=your_phantom_wallet_address_here

# Base Chain
BASE_ADDRESS=your_base_wallet_address_here

# Starknet (Ready.co)
STARKNET_RPC_URL=https://starknet-mainnet.public.blastapi.io
STARKNET_PRIVATE_KEY=0x...
STARKNET_ACCOUNT_ADDRESS=0x...
STARKNET_ADDRESS=your_starknet_address_here

# Security
VAULT_PASSWORD=secret_password_here

# Optional: External APIs
INFLUENCE_API_URL=https://api.influence.eth
COINGECKO_API_KEY=your_coingecko_api_key_here
JUPITER_API_KEY=your_jupiter_api_key_here
